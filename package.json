{
  "name": "relay-starter",
  "version": "0.1.0",
  "description": "React -> Relay starter kit",
  "main": "src/index.jsx",
  "scripts": {
    "preinstall": "autod",
    "pretest": "npm run clean && npm run lint",
    "clean": "npx rimraf coverage dist",
    "lint": "npx eslint --ext .jsx,.js src",
    "build": "NODE_ENV=production npx parcel build --no-cache public/index.html --public-url ./",
    "start": "NODE_ENV=development npx parcel --no-cache public/index.html",
    "test": "cross-env NODE_ENV=test jest --no-cache"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jimzhan/relay-starter.git"
  },
  "keywords": [
    "react",
    "relay",
    "antd",
    "graphql",
    "boilerplate",
    "starter",
    "kit",
    "tools",
    "skeleton"
  ],
  "author": "Jim Zhan <jim.zhan@me.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/jimzhan/relay-starter/issues"
  },
  "homepage": "https://github.com/jimzhan/relay-starter#readme",
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">1%",
      "last 4 versions",
      "Firefox ESR",
      "not ie < 11"
    ]
  }
}
